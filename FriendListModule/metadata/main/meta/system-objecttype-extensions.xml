                        <attribute attribute-id="GCLimitMax"/>
                        <attribute attribute-id="GCLimit"/>
                        <attribute attribute-id="GCValueCode"/>
                        <attribute attribute-id="GCLimitMin"/>
                        <attribute attribute-id="GCCardSetId"/>
                        <attribute attribute-id="GCPinActivateField"/>
                        <attribute attribute-id="GCPinBypassRegex"/>
                        <attribute attribute-id="Clutch_isLoyaltyEnabled"/>
                        <attribute attribute-id="LCCardSetId"/>
                        <attribute attribute-id="LCPinActivateField"/>
                        <attribute attribute-id="LCValueCode"/>
                        <attribute attribute-id="Clutch_checkoutLock"/>
                        <attribute attribute-id="Clutch_loyaltyRedemption"/>
                        <attribute attribute-id="Clutch_recaptchaSiteKey"/>
